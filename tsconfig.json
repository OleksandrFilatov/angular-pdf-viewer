{
  "compilerOptions": {
    "noImplicitAny": false,
    "module": "system",
    "target": "es5",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "declaration": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "rootDir": ".",
    "inlineSourceMap": true,
    "inlineSources": true,
    "stripInternal": true,
    "noEmitOnError": false
  },
  "exclude": [
    "node_modules",
    "typings",
    "dist"
  ]
}